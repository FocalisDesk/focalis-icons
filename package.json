{
  "name": "@focalisdesk/focalis-icons",
  "version": "1.0.0",
  "description": "A beautiful set of SVG icons in two styles: regular-rounded and thin-rounded. Easy to use as web fonts or SVGs with CDN support.",
  "main": "dist/css/focalis_regular_rounded.css",
  "style": "dist/css/focalis_regular_rounded.css",
  "files": [
    "dist",
    "manifest.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "generate:codepoints": "node font-build/generate-codepoints.js",
    "prebuild:regular": "mkdir -p dist/fonts/regular-rounded dist/css dist/data",
    "build:regular": "fantasticon --config font-build/configs/focalis-regular-rounded.js",
    "prebuild:thin": "mkdir -p dist/fonts/thin-rounded dist/css dist/data",
    "build:thin": "fantasticon --config font-build/configs/focalis-thin-rounded.js",
    "build:all": "npm run build:regular && npm run build:thin",
    "build": "npm run clean && npm run build:all",
    "minify:css": "node font-build/minify-css.js",
    "postbuild": "npm run minify:css",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FocalisDesk/focalis-icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "font",
    "icon-font",
    "web-font",
    "regular-rounded",
    "thin-rounded",
    "focalis",
    "ui",
    "design-system"
  ],
  "author": "FocalisDesk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FocalisDesk/focalis-icons/issues"
  },
  "homepage": "https://github.com/FocalisDesk/focalis-icons#readme",
  "engines": {
    "node": ">=25",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "fantasticon": "4.1.0",
    "clean-css-cli": "^5.6.3",
    "rimraf": "^5.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}